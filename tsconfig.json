{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./src/assets/*"
      ],
      "@components/*":[
        "./src/components/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@theme/*":[
        "./src/theme/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],     
      "@storage/*":[
        "./src/storage/*"
      ],           
    }
  }
}